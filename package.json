{
  "name": "app-ai-chatbot",
  "version": "0.0.1",
  "description": "Simple chatbot, based on rasa framework",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/enonic/app-ai-chatbot.git"
  },
  "scripts": {
    "build": "gradle build",
    "deploy": "gradle deploy",
    "watch": "gradle deploy -t",
    "train-nlu": "cd rasa && python bot.py train-nlu",
    "train-stories": "cd rasa && python bot.py train-stories",
    "rasa-server": "cd rasa && python -m rasa_core.server -d models/stories -u models/nlu/default/current -p 7454 --cors [*] -o logs/server.log --debug -v",
    "lint:js": "eslint \"src/main/resources/**/*.js\" --debug",
    "lint:css": "stylelint \"src/**/*.css\"",
    "lint": "run-p -c lint:css lint:js"
  },
  "keywords": [
    "enonic",
    "chatbot"
  ],
  "author": "Mikita Taukachou <edloidas@gmail.com> (https://edloidas.com)",
  "contributors": [
    "Alex Shklianko <asl@enonic.com> (https://enonic.com)",
    "Pavel Milkevich <pmi@enonic.com> (https://enonic.com)",
    "Viachaslau Bradnitski <vbr@enonic.com> (https://enonic.com)"
  ],
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/enonic/app-ai-chatbot/issues"
  },
  "homepage": "https://github.com/enonic/app-ai-chatbot#readme",
  "devDependencies": {
    "autoprefixer": "8.1.0",
    "babel-core": "^6.26.0",
    "babel-loader": "7.1.4",
    "babel-minify-webpack-plugin": "^0.3.0",
    "babel-preset-env": "^1.6.1",
    "browserslist": "^3.1.2",
    "browserslist-config-enonic": "^1.0.3",
    "cross-env": "5.1.4",
    "css-loader": "0.28.11",
    "css-mqpacker": "6.0.2",
    "cssnano": "4.0.0-rc.2",
    "deep-assign": "^2.0.0",
    "download": "^6.2.5",
    "eslint": "4.19.0",
    "eslint-config-airbnb": "16.1.0",
    "eslint-config-airbnb-base": "12.1.0",
    "eslint-config-prettier": "2.9.0",
    "eslint-plugin-flowtype": "2.46.1",
    "eslint-plugin-import": "2.9.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-prettier": "2.6.0",
    "eslint-plugin-react": "7.7.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "husky": "0.14.3",
    "lint-staged": "7.0.0",
    "less": "^3.0.1",
    "less-loader": "^4.1.0",
    "npm-run-all": "^4.1.2",
    "postcss-calc": "^6.0.1",
    "postcss-discard-comments": "^2.0.4",
    "postcss-import": "11.1.0",
    "postcss-loader": "2.1.1",
    "postcss-mixins": "^6.2.0",
    "postcss-nested": "^3.0.0",
    "postcss-normalize": "^4.0.0",
    "postcss-simple-vars": "^4.1.0",
    "prettier": "1.11.1",
    "style-loader": "0.20.3",
    "stylelint": "9.1.3",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-processor-styled-components": "1.3.1",
    "webpack": "3.11.0",
    "webpack-dev-server": "2.11.2",
    "workbox-webpack-plugin": "^2.1.0"
  },
  "browserslist": [
    "firefox >= 58",
    "chrome >= 63"
  ],
  "engines": {
    "node": ">= 8.2.1",
    "npm": ">= 5.3.0"
  }
}
